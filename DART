import 'dart:html';

void main(){
  final InputElement input = querySelector('input');
  final DivElement div = querySelector('div');
  
  input.onInput
    .listen((dynamic event)=> print(event.target.value));//使用dynamic等於把event的類型改成dynamic，所以即使eventtarget沒有value這個屬性也不會出錯
}


//ps
//此event原為Event類型,onInput返回 ElementStream<Event>類型 兩個並不相等
//像ElementStream<Event>之類的泛型不可使用<>裡(Event)的屬性
